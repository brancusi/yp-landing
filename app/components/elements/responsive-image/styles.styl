& {
  height: 100%;
  width: 100%;
}

img {
  object-fit: contain;
}

.img-container.contain {

  img {
    object-fit: contain;
  }
}

.img-container.cover {
  img {
    object-fit: cover;
  }
}

.img-container {
  height: 100%;
  width: 100%;
  overflow: hidden;
}

img {
  width: 100%;
  height: 100%;

  object-position: center;
}
